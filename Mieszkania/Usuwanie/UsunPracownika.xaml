<UserControl x:Class="Mieszkania.UsunPracownika"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mieszkania"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <Button x:Name="btn_Wybierz" Content="Wybierz Pracownika" HorizontalAlignment="Left" VerticalAlignment="Top" Width="204" Margin="90,28,0,0" Height="53" Click="btn_Wybierz_Click"/>
        <Button x:Name="btn_Usun" Content="Usun" HorizontalAlignment="Left" VerticalAlignment="Top" Width="204" Margin="90,232,0,0" Height="50" Click="btn_Usun_Click"/>
        <TextBox x:Name="txt_id"  IsEnabled="False" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top" Width="120" Margin="174,140,0,0"/>
        <Label Content="ID pracownika: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="90,136,0,0"/>
    </Grid>
</UserControl>
