<UserControl x:Class="Mieszkania.UsunLokatora"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mieszkania"
        mc:Ignorable="d"
         Height="450" Width="600" Background="White">
    <Grid>
        <Button x:Name="btn_Wybierz" Content="Wybierz Lokatora" HorizontalAlignment="Left" VerticalAlignment="Top" Width="204" Margin="90,28,0,0" Height="53" Click="btn_Wybierz_Click"/>
        <Button x:Name="btn_Usun" Content="Usun" HorizontalAlignment="Left" VerticalAlignment="Top" Width="204" Margin="90,232,0,0" Height="50" Click="btn_Usun_Click"/>
        <TextBox x:Name="txt_id" IsEnabled="False" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top" Width="120" Margin="174,140,0,0"/>
        <Label Content="ID lokatora: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="90,136,0,0"/>
    </Grid>
</UserControl>
