<UserControl x:Class="Mieszkania.Wyswietlanie.Kon_WyswietlUmowy"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mieszkania.Wyswietlanie"
        mc:Ignorable="d"
        Height="450" Width="800">
    <Grid Background="White">
        <DataGrid x:Name="dataG" HorizontalAlignment="Left" Height="356" Margin="10,84,0,0" VerticalAlignment="Top" Width="772"/>
        <TextBox x:Name="txt_imi" HorizontalAlignment="Left" Height="30" Margin="12,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="179" TextChanged="txt_imi_TextChanged"/>
        <TextBox x:Name="txt_Naz" HorizontalAlignment="Left" Height="30" Margin="217,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="179" TextChanged="txt_Naz_TextChanged"/>
        <Label Content="Imię:" HorizontalAlignment="Left" Height="34" Margin="12,10,0,0" VerticalAlignment="Top" Width="181"/>
        <Label Content="Nazwisko:" HorizontalAlignment="Left" Height="34" Margin="217,10,0,0" VerticalAlignment="Top" Width="181"/>

    </Grid>
</UserControl>
