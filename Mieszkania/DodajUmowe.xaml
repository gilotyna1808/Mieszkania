<UserControl x:Class="Mieszkania.DodajUmowe"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mieszkania"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="600">
    <Grid Background="White">
        <Label Content="Od Kiedy" HorizontalAlignment="Left" Height="32" Margin="63,65,0,0" VerticalAlignment="Top" Width="168"/>
        <Label Content="Do Kiedy" HorizontalAlignment="Left" Height="32" Margin="63,102,0,0" VerticalAlignment="Top" Width="168"/>
