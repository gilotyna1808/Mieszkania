        <Button x:Name="btn_dodaj" Content="Dodaj" HorizontalAlignment="Left" Height="32" Margin="226,391,0,0" VerticalAlignment="Top" Width="152" Click="btn_dodaj_Click"/>

    </Grid>
</UserControl>
