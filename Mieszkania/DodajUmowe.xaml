        <Label Content="Id Mieszkania" HorizontalAlignment="Left" Height="32" Margin="63,250,0,0" VerticalAlignment="Top" Width="168"/>
        <Label Content="Id Lokatora" HorizontalAlignment="Left" Height="32" Margin="63,287,0,0" VerticalAlignment="Top" Width="168"/>
        <TextBox x:Name="txt_data_p" HorizontalAlignment="Left" Height="32" Margin="268,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="165"/>
        <TextBox x:Name="txt_data_k" HorizontalAlignment="Left" Height="32" Margin="268,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="165"/>
        <TextBox x:Name="txt_czynsz" HorizontalAlignment="Left" Height="32" Margin="268,129,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="165"/>
